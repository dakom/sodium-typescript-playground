{
  "name": "sodium-typescript-playground",
  "version": "1.0.0",
  "description": "Sodium Typescript Playground",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "test": "cross-env NODE_ENV=test node fuse.js",
    "build": "npm run clean && npm run _build:dev && npm run _build:prod && npm run _build:tsc",
    "_build:tsc": "tsc && mv ./dist/tsc/typings ./dist && rimraf ./dist/tsc",
    "_build:dev": "cross-env NODE_ENV=dev node fuse.js",
    "_build:prod": "cross-env NODE_ENV=production node fuse.js",
    "server:prod": "http-server ./dist -p 5000 --cors",
    "_localstatic": "http-server ./static -p 4000 --cors"
  },
  "author": "David Komer",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/pixi.js": "4.5.1",
    "cross-env": "^5.0.5",
    "fuse-box": "^2.2.2",
    "fuse-test-runner": "^1.0.13",
    "http-server": "^0.10.0",
    "npm-run-all": "^4.0.2",
    "ramda": "0.24.1",
    "rimraf": "2.6.1",
    "sodiumjs": "git+https://github.com/SodiumFRP/sodium-typescript.git",
    "typescript": "^2.4.2",
    "typescript-collections": "1.2.3",
    "uglify-js": "^3.0.28"
  },
  "dependencies": {}
}
