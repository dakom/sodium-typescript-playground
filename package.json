    "install": "^0.10.1",
    "npm-run-all": "^4.0.2",
    "ramda": "0.24.1",
    "rimraf": "2.6.1",
    "sodiumjs": "git+https://github.com/SodiumFRP/sodium-typescript.git",
    "typescript": "^2.4.2",
